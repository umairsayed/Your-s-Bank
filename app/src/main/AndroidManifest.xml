<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yoursbank">

    <application
        android:allowBackup="true"
        android:icon="@drawable/bank_logo3"
        android:label="@string/app_name"
        android:roundIcon="@drawable/bank_logo3"
        android:supportsRtl="true"
        android:theme="@style/Theme.YoursBank">
        <activity android:name="com.lockhart.yoursbank.CheckTransaction" />
        <activity android:name="com.lockhart.yoursbank.EmpTransActivity" />

        <activity
            android:name="com.lockhart.yoursbank.LocActivity"
            android:exported="true" />
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/map_key" />


        <activity android:name="com.lockhart.yoursbank.SendMoneyActivity" />
        <activity android:name="com.lockhart.yoursbank.EmployeeDashboardActivity" />
        <activity android:name="com.lockhart.yoursbank.CustomerDashboardActivity" />
        <activity android:name="com.lockhart.yoursbank.RegistrationActivity" />
        <activity android:name="com.lockhart.yoursbank.LoginActivity" />
        <activity android:name="com.lockhart.yoursbank.LauncherActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>